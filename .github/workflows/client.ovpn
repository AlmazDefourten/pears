client
dev tun
proto tcp
remote vpn.kai.ru

resolv-retry infinite
nobind
persist-key
persist-tun

auth-user-pass
auth-retry interact
auth-nocache

remote-cert-tls server
verify-x509-name vpn.kai.ru name
cipher none

setenv opt block-outside-dns
route-method exe
route-delay 2
redirect-gateway def1

<connection>
  proto pptp
  name KAI-VPN
  require-mschap-v2
  refuse-eap
  nobsdcomp
  nodeflate
  mtu-test
  mms-fix
  nopcomp
  noaccomp
  unit 1
</connection>
